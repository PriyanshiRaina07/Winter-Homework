import java.util.*;
public class HelloWorld {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        int q = sc.nextInt();

        long[] prefix = new long[n + 1];

        for(int i = 1; i <= n; i++){
            prefix[i] = prefix[i - 1] + sc.nextInt();   //adding prefix 
        }
        while (q-- > 0) {
            int a = sc.nextInt();
            int b = sc.nextInt();
            System.out.println(prefix[b] - prefix[a - 1]);
        }
        
    }
}
       


